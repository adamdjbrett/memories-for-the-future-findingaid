{
  "name": "11ty-research",
  "version": "1.0.1",
  "description": "Innovation from Adam DJ Brett for ease in developing the libguide project",
  "type": "module",
  "scripts": {
    "build": "rm -rf _site && eleventy && pagefind --site _site --glob \"**/*.html\"",
    "build-nocolor": "cross-env NODE_DISABLE_COLORS=1 eleventy",
    "build-ghpages": "rm -rf _site && eleventy --pathprefix=/11ty-research-guide/ && pagefind --site _site --glob \"**/*.html\"",
    "start": "eleventy --serve --quiet",
    "start-ghpages": "eleventy --pathprefix=/11ty-research-guide/ --serve --quiet",
    "debug": "cross-env DEBUG=Eleventy* eleventy",
    "debugstart": "cross-env DEBUG=Eleventy* eleventy --serve --quiet",
    "benchmark": "cross-env DEBUG=Eleventy:Benchmark* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamdjbrett/11ty-research-guide.git"
  },
  "author": {
    "name": "Adam Dj Brett",
    "email": "info@adamdjbrett.com",
    "url": "https://adamdjbrett.com/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^5.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "cross-env": "^7.0.3",
    "js-yaml": "^4.1.0",
    "luxon": "^3.5.0",
    "markdown-it-attrs": "^4.3.1",
    "pagefind": "^1.4.0",
    "purgecss": "^6.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.1"
  },
  "dependencies": {
    "@11ty/eleventy-fetch": "^5.0.1",
    "@zachleat/heading-anchors": "^1.0.1",
    "cheerio": "^1.2.0",
    "citation-js": "^0.7.16",
    "clean-css": "^5.3.3",
    "csv-parse": "^6.1.0",
    "csvtojson": "^2.0.14",
    "eleventy-plugin-pwa-v2": "^1.0.2",
    "eleventy-plugin-toc": "^1.1.5",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-table-of-contents": "^0.8.0",
    "node-fetch": "^3.3.2",
    "undici": "^7.21.0"
  }
}
